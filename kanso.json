{
  "name": "its",
  "version": "0.0.1",
  "description": "Lupo Libero's Issue Tracking System",
  "load": "src/ITS/app",
  "modules": ["src"],
  "attachments": ["static", "partials"],
  "shell": [
    "grunt compile",
    "coffee ./bots/external_tools.coffee default"
  ],
  "bots": [
    "node ./bots/role_setter.js",
    "node ./bots/mailer.js",
    "node ./bots/notification_creator.js"
  ],
  "dependencies": {
    "properties": null,
    "modules": null,
    "attachments": null,
    "base64": null,
    "couchtypes": null,
    "blocking-multi-shell": null,
    "md5": null
  }
}
